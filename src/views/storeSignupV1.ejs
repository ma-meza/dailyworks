function submitRegisterForm(){
  var labelsUnderInput = document.getElementsByClassName('inputUnderText');
  for(var i=0;i<labelsUnderInput.length;i++){
    labelsUnderInput[i].innerText = "";
  }


  var emailSignup = $("#storeSignupEmail").val();
  var password = $("#storeSignupPassword").val();
  var name = $("#storeSignupName").val();
  var type = $("#storeSignupType").val();
  var planStripeId = $("#membPlanId").val();
  console.log(planStripeId);
  console.log(password);

  if(emailSignup!="" && password!="" && name!=""){
    //all fields are full
    if(password.length>=5 && name.length>=3){
      //ALL FIElds have good length

      stripe.createToken(card).then(function(resultToken) {
        if (resultToken.error) {
          var errorElement = document.getElementById('card-errors');
          errorElement.textContent = resultToken.error.message;
        } else {
          $.ajax({
                url: '/postLocalStoreSignup',
                type: 'post',
                data: {stripeToken:resultToken.token, email:emailSignup, customerEmail: emailSignup, password:password, storeName:name, storeType:type, planId:planStripeId},
                success: function( data, textStatus){
                  if(data == "ok"){
                    location.href = "/storeLogin";
                  }else if(data=="alreadyExist"){
                    alert('Store already exists');

                  }else if(data=="err"){
                    console.log('errrrrrrr');
                  }else{

                  }
                },
                error: function( textStatus, errorThrown ){
                    if(textStatus=="alreadyExist"){

                    }else if(textStatus=="err"){

                    }else{

                    }
                }
            });
        }
      })


    }else{
      //not all fields have good length
      if(password.length<5){
        document.getElementById('storeSignupPassword').focus();
        document.getElementById("passwordUnderText").innerText = "Your password must be at least 5 characters";
        displaySpecificDiv(1);
      }else if(name.length<5){
        document.getElementById('storeSignupName').focus();
        document.getElementById("nameUnderText").innerText = "Your name must be at least 5 characters";
        displaySpecificDiv(2);
      }
    }

  }else{
    //one or more field is empty
    if(emailSignup ==""){
      document.getElementById('storeSignupEmail').focus();
      document.getElementById("emailUnderText").innerText = "Please write your email";
      displaySpecificDiv(0);
    }else if(password ==""){
      document.getElementById('storeSignupPassword').focus();
      document.getElementById("passwordUnderText").innerText = "Please write your password";
      displaySpecificDiv(1);
    }else if(name ==""){
      document.getElementById('storeSignupName').focus();
      document.getElementById("nameUnderText").innerText = "Please write your name";
      displaySpecificDiv(2);
    }
  }
}



document.getElementById("submitButtonRegister").onclick = function(){
