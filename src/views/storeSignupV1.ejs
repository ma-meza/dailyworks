  var paymentValueDiv = document.getElementById('paymentInfoMainDiv');
  var submitButtonRegister = document.getElementById('submitButtonRegister');
  var nextButtonRegister = document.getElementById('nextButtonRegister');

  submitButtonRegister.style.display = "none";
  nextButtonRegister.style.display = "block";
  emailValueDiv.className = "singleInputMainDiv";
  passwordValueDiv.className = "singleInputMainDiv";
  bnameValueDiv.className = "singleInputMainDiv";
  btypeValueDiv.className = "singleInputMainDiv";
  paymentValueDiv.className = "singleInputMainDiv";
  planSelectionDiv.className = "singleInputMainDiv";

  switch(divNumber){
    case 0:
      document.getElementById('chevronLeftBack').style.display = "none";
      emailValueDiv.classList.add('activeSingleInputMainDiv');
      registrationStepValue = 0;
    break;
    case 1:
      document.getElementById('chevronLeftBack').style.display = "block";
      registrationStepValue = 1;
      emailValueDiv.classList.add('leftSingleInputMainDiv');
      passwordValueDiv.classList.add("activeSingleInputMainDiv");
    break;
    case 2:
      document.getElementById('chevronLeftBack').style.display = "block";
      registrationStepValue = 2;
      emailValueDiv.classList.add('leftSingleInputMainDiv');
      passwordValueDiv.classList.add('leftSingleInputMainDiv');
      bnameValueDiv.classList.add('activeSingleInputMainDiv');
    break;
    case 3:
      document.getElementById('chevronLeftBack').style.display = "block";
      registrationStepValue = 3;
      emailValueDiv.classList.add('leftSingleInputMainDiv');
      passwordValueDiv.classList.add('leftSingleInputMainDiv');
      bnameValueDiv.classList.add('leftSingleInputMainDiv');

      btypeValueDiv.classList.add('activeSingleInputMainDiv');

    break;
    case 4:
    break;
    case 5:
