var planNameContainer = document.getElementsByClassName('planNameContainer');
for(var i=0;i<planNameContainer.length;i++){
  planNameContainer[i].onclick = function(){
    if(!this.classList.contains('activePlanNameContainer')){
      setPlanIconsInactive();
      document.getElementById('membPlanId').value=this.getAttribute('data-planid');
      var membershipId = this.getAttribute('data-memId');
      this.classList.add('activePlanNameContainer');
      setPlanIconsActive(membershipId);
    }
  }
}

function setPlanIconsInactive(){
  var activePlanNameContainer = document.getElementsByClassName('activePlanNameContainer')[0].classList.remove('activePlanNameContainer');
  var activePricePlanP = document.getElementsByClassName('activePricePlan');
  var activeCheckIcon = document.getElementsByClassName('activeCheckIcon');
  var activeXIcon = document.getElementsByClassName('activeXIcon');

  while(activeCheckIcon.length>0){
    activeCheckIcon[0].src="assets/icons/octiconsSVG/check-inactive.svg";
    activeCheckIcon[0].classList.remove('activeCheckIcon');
  }
  while(activeXIcon.length>0){
    activeXIcon[0].src="assets/icons/octiconsSVG/x-inactive.svg";
    activeXIcon[0].classList.remove('activeXIcon');
  }
  while(activePricePlanP.length>0){
    activePricePlanP[0].classList.remove('activePricePlan');
  }
}


function setPlanIconsActive(membershipId){
var planPriceP = document.getElementsByClassName('planPriceP');
var xIconPlan = document.getElementsByClassName('xIconPlan');
var checkIconPlan = document.getElementsByClassName('checkIconPlan');
for(var t=0;t<xIconPlan.length;t++){
  if(xIconPlan[t].getAttribute('data-memId') == membershipId){
    xIconPlan[t].classList.add('activeXIcon');
    xIconPlan[t].src="assets/icons/octiconsSVG/x-active.svg";
  }
}
for(var t=0;t<checkIconPlan.length;t++){
  if(checkIconPlan[t].getAttribute('data-memId') == membershipId){
    checkIconPlan[t].classList.add('activeCheckIcon');
    checkIconPlan[t].src="assets/icons/octiconsSVG/check-active.svg";
  }
}
for(var t=0;t<planPriceP.length;t++){
  if(planPriceP[t].getAttribute('data-memId') == membershipId){
    planPriceP[t].classList.add('activePricePlan');
  }
}
}


var registrationStepValue = 0;

document.getElementById('nextButtonRegister').onclick = function(){
  nextRegisterStep();
};

function nextRegisterStep(){
  switch(registrationStepValue){
    case 0:
      document.getElementById('chevronLeftBack').style.display = "block";
      var emailValueDiv = document.getElementById('emailValueDiv');
      var passwordValueDiv = document.getElementById('passwordValueDiv');
      emailValueDiv.classList.remove('activeSingleInputMainDiv');
      emailValueDiv.classList.add('leftSingleInputMainDiv');
      passwordValueDiv.classList.add('activeSingleInputMainDiv');
      registrationStepValue++;
    break;
    case 1:
      document.getElementById('chevronLeftBack').style.display = "block";
      var passwordValueDiv = document.getElementById('passwordValueDiv');
      var bnameValueDiv = document.getElementById('bnameValueDiv');
      passwordValueDiv.classList.remove('activeSingleInputMainDiv');
      passwordValueDiv.classList.add('leftSingleInputMainDiv');
      bnameValueDiv.classList.add('activeSingleInputMainDiv');
      registrationStepValue++;
    break;
    case 2:
      document.getElementById('chevronLeftBack').style.display = "block";
      var bnameValueDiv = document.getElementById('bnameValueDiv');
      var btypeValueDiv = document.getElementById('btypeValueDiv');
      bnameValueDiv.classList.remove('activeSingleInputMainDiv');
      bnameValueDiv.classList.add('leftSingleInputMainDiv');
      btypeValueDiv.classList.add('activeSingleInputMainDiv');
      registrationStepValue++;
    break;
    case 3:
    document.getElementById('chevronLeftBack').style.display = "block";
    var btypeValueDiv = document.getElementById('btypeValueDiv');
    var bplanValueDiv = document.getElementById('planSelectionDiv');
    btypeValueDiv.classList.remove('activeSingleInputMainDiv');
    btypeValueDiv.classList.add('leftSingleInputMainDiv');
    bplanValueDiv.classList.add('activeSingleInputMainDiv');
    registrationStepValue++;
    break;
    case 4:
    document.getElementById('chevronLeftBack').style.display = "block";
    var bplanValueDiv = document.getElementById('planSelectionDiv');
    var bcardinfoValueDiv = document.getElementById('paymentInfoMainDiv');
    bplanValueDiv.classList.remove('activeSingleInputMainDiv');
    bplanValueDiv.classList.add('leftSingleInputMainDiv');
    bcardinfoValueDiv.classList.add('activeSingleInputMainDiv');
    registrationStepValue++;

    document.getElementById('nextButtonRegister').style.display = "none";
    document.getElementById('submitButtonRegister').style.display = "block";
    break;
  }
  console.log(registrationStepValue);
}


function backRegisterStep(){
  switch(registrationStepValue){
    case 1:
      document.getElementById('chevronLeftBack').style.display = "none";
      var passwordValueDiv = document.getElementById('passwordValueDiv');
      var emailValueDiv = document.getElementById('emailValueDiv');
      passwordValueDiv.classList.remove('activeSingleInputMainDiv');
      emailValueDiv.classList.add('activeSingleInputMainDiv');
      emailValueDiv.classList.remove('leftSingleInputMainDiv');
      registrationStepValue--;
    break;
    case 2:
      var bnameValueDiv = document.getElementById('bnameValueDiv');
      var passwordValueDiv = document.getElementById('passwordValueDiv');
      passwordValueDiv.classList.remove('leftSingleInputMainDiv');
      passwordValueDiv.classList.add('activeSingleInputMainDiv');
      bnameValueDiv.classList.remove('activeSingleInputMainDiv');
      registrationStepValue--;
    break;
    case 3:
      var bnameValueDiv = document.getElementById('bnameValueDiv');
      var btypeValueDiv = document.getElementById('btypeValueDiv');
      bnameValueDiv.classList.add('activeSingleInputMainDiv');
      bnameValueDiv.classList.remove('leftSingleInputMainDiv');
      btypeValueDiv.classList.remove('activeSingleInputMainDiv');
      registrationStepValue--;
    break;
    case 4:
      var bplanDiv = document.getElementById('planSelectionDiv');
      var btypeValueDiv = document.getElementById('btypeValueDiv');
      btypeValueDiv.classList.add('activeSingleInputMainDiv');
      btypeValueDiv.classList.remove('leftSingleInputMainDiv');
      bplanDiv.classList.remove('activeSingleInputMainDiv');
      registrationStepValue--;
    break;
    case 5:
      var bcardinfoValueDiv = document.getElementById('paymentInfoMainDiv');
      var bplanValueDiv = document.getElementById('planSelectionDiv');
      bplanValueDiv.classList.add('activeSingleInputMainDiv');
      bplanValueDiv.classList.remove('leftSingleInputMainDiv');
      bcardinfoValueDiv.classList.remove('activeSingleInputMainDiv');
      registrationStepValue--;
      document.getElementById('nextButtonRegister').style.display = "block";
      document.getElementById('submitButtonRegister').style.display = "none";
    break;

  }
}
